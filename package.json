{
  "name": "@agient/root",
  "displayName": "AGIent",
  "version": "0.0.1",
  "private": true,
  "description": "An AI chatbot that will perform actions on the behalf of users to help them get things done",
  "author": "Eirik Måseidvåg",
  "workspaces": [
    "app",
    "widget",
    "backend"
  ],
  "scripts": {
    "build": "nx run-many -t build --parallel 10",
    "build:app": "nx build app",
    "build:widget": "nx build widget",
    "build:backend": "nx build backend",
    "generate-types": "swagger-typescript-api -p http://localhost:3000/docs-json -o apps/app/src/services/api --modular --extract-enums --single-http-client --axios -n ./types.ts --templates src/templates --type-prefix Api --clean-output",
    "kill": "lsof -i :8080 | awk 'FNR == 2 {print $2}' | xargs kill -9",
    "lint": "nx run-many -t lint,stylelint --parallel 10 && nx format:check --all",
    "lint:fix": "nx run-many -t lint:fix,stylelint:fix --parallel 10 && nx format:write --all",
    "serve:app": "nx serve app",
    "serve:widget": "nx serve widget --watch",
    "serve:backend": "nx serve backend",
    "test": "nx run-many -t test --parallel 10",
    "migrations:generate": "nx migrations:generate backend",
    "migrations:run": "nx migrations:run backend",
    "migrations:revert": "nx migrations:revert backend",
    "migrations:show": "nx migrations:show backend",
    "schema:drop": "nx schema:drop backend"
  },
  "dependencies": {
    "@nestjs/axios": "^3.0.2",
    "@nestjs/common": "^10.3.6",
    "@nestjs/config": "^3.2.1",
    "@nestjs/core": "^10.3.6",
    "@nestjs/jwt": "^10.2.0",
    "@nestjs/microservices": "^10.3.6",
    "@nestjs/platform-express": "^10.3.6",
    "@nestjs/platform-socket.io": "^10.3.6",
    "@nestjs/swagger": "^7.3.0",
    "@nestjs/typeorm": "^10.0.2",
    "@nestjs/websockets": "^10.3.6",
    "@supabase/supabase-js": "^2.40.0",
    "@tanstack/vue-query": "^5.28.9",
    "@vueuse/core": "^10.9.0",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.1",
    "code-tag": "^1.2.0",
    "crypto-js": "^4.2.0",
    "es6-shim": "^0.35.8",
    "openai": "^4.29.2",
    "pg": "^8.11.3",
    "pinia": "^2.1.7",
    "primevue": "^3.50.0",
    "reflect-metadata": "^0.2.1",
    "rxjs": "^7.8.1",
    "socket.io-client": "^4.7.5",
    "swagger-ui-express": "^5.0.0",
    "tslib": "^2.6.2",
    "typeorm": "^0.3.20",
    "uuid": "^9.0.1",
    "vue": "^3.4.21",
    "vue-router": "^4.3.0"
  },
  "devDependencies": {
    "@nestjs/schematics": "^10.1.1",
    "@nestjs/testing": "^10.3.6",
    "@nx/eslint": "^18.1.3",
    "@nx/eslint-plugin": "^18.1.3",
    "@nx/jest": "^18.1.3",
    "@nx/js": "^18.1.3",
    "@nx/nest": "^18.1.3",
    "@nx/node": "^18.1.3",
    "@nx/vite": "^18.1.3",
    "@nx/webpack": "^18.1.3",
    "@nx/workspace": "^18.1.3",
    "@pinia/testing": "^0.1.3",
    "@types/crypto-js": "^4.2.2",
    "@types/jest": "^29.5.12",
    "@types/json-schema": "^7.0.15",
    "@types/jsonwebtoken": "^9.0.6",
    "@types/node": "20.11.30",
    "@types/sinon": "^17.0.3",
    "@types/uuid": "^9.0.8",
    "@typescript-eslint/eslint-plugin": "^7.4.0",
    "@typescript-eslint/parser": "^7.4.0",
    "@vitejs/plugin-vue": "^5.0.4",
    "@vue/eslint-config-typescript": "^13.0.0",
    "@vue/test-utils": "^2.4.5",
    "autoprefixer": "^10.4.19",
    "axios": "^1.6.8",
    "eslint": "8.57.0",
    "eslint-config-prettier": "9.1.0",
    "eslint-plugin-import": "^2.29.1",
    "eslint-plugin-vue": "^9.24.0",
    "happy-dom": "^14.3.9",
    "jest": "^29.7.0",
    "jest-environment-node": "^29.7.0",
    "jest-mock-extended": "^3.0.5",
    "nx": "^18.1.3",
    "nx-stylelint": "^17.1.4",
    "postcss": "^8.4.38",
    "postcss-html": "^1.6.0",
    "prettier": "^3.2.5",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "sass": "^1.72.0",
    "sinon": "^17.0.1",
    "socket.io": "^4.7.5",
    "stylelint": "^16.3.1",
    "stylelint-config-recommended-vue": "^1.5.0",
    "stylelint-config-standard-scss": "^13.0.0",
    "swagger-typescript-api": "^13.0.3",
    "tailwindcss": "3.4.1",
    "ts-jest": "^29.1.2",
    "ts-node": "~10.9.2",
    "tsconfig": "^7.0.0",
    "typescript": "~5.4.3",
    "vite": "^5.2.6",
    "vite-plugin-css-injected-by-js": "^3.5.0",
    "vite-plugin-dts": "^3.7.3",
    "vite-tsconfig-paths": "^4.3.2",
    "vitest": "^1.4.0",
    "vue-eslint-parser": "^9.4.2",
    "vue-tsc": "^2.0.7"
  },
  "engines": {
    "node": ">=20.11.0",
    "npm": ">=10.2.4",
    "pnpm": ">=8.6.5"
  }
}
